import 'child', [:compile, :run_basic], File.join(File.dirname(__FILE__), "child.avj")

job :compile do
  execute "avsim --unit cvac --compile"
end

job :run_basic, :compile do
  execute "avsim --unit cvac --run --test cvac_basic_test"
end

job :run_random, :compile do
  execute "avsim --unit cvac --run --test cvac_random_test"
  repeat 10
end
