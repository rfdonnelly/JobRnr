import "grandchild", [:compile], File.join(File.dirname(__FILE__), "grandchild.avj")

job :compile do
  execute "avsim -u hk --compile"
end

job :run_basic, :compile do
  execute "avsim --unit hk --run --test hk_basic_test"
end

job :run_random, :compile do
  execute "avsim --unit hk --run --test hk_random_test"
  repeat 10
end

